<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>SQuizzes</title>
    <link rel="icon" href="/favicon.svg">
    <meta name="description" content="SQuizzes is an application where you can easily create your own SFX quiz and share it with friends or just use and try to guess other users' quizzes." />
    @RenderSection("Styles", false)
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="preload" as="style" onload="this.rel='stylesheet'" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="preload" as="style" onload="this.rel='stylesheet'" href="~/styles/css/main.min.css" />
    <script defer src="https://kit.fontawesome.com/53d9947631.js" crossorigin="anonymous"></script>
</head>
<body>
    @Html.Partial("_NavbarPartial")

    <main>
        @RenderBody()
    </main>

    @Html.Partial("_FooterPartial")


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>